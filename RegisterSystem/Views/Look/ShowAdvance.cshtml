
@{
    ViewBag.Title = "预约登记记录";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <h2 class="text-center">预约登记记录</h2>
    <form class="form-inline" style="margin:50px;">
        <div class="form-group">
            <label for="nameID">姓名</label>
            <input type="text" class="form-control" id="nameID" name="name">
        </div>
        <div class="form-group" style="margin-left:20px;">
            <label for="beginTimeID">开始时间</label>
            <input type="datetime-local" class="form-control" id="beginTimeID" name="beginTime">
        </div>
        <div class="form-group " style="margin-left:20px;">
            <label for="endTimeID">结束时间</label>
            <input type="datetime-local" class="form-control" id="endTimeID" name="endTime">
        </div>
        <button id="btn" type="button" class="btn btn-primary" style="margin-left:20px;">确定</button>
    </form>

    <table class="table table-striped" id="table"></table>

    <nav>
        <ul class="pager" id="page"></ul>
    </nav>


</div>


@section scripts {
    <script src="~/Scripts/AdvanceJS.js"></script>
}