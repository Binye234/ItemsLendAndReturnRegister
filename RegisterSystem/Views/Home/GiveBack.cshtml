
@{
    ViewBag.Title = "归还登记";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row">
        <h3 class="text-center" style="padding-right:80px">归还登记</h3>
        <div class="col-md-4 col-md-offset-4">

            <form action="@Url.Action("RegistrationGiveBack")" method="post">
                <div class="form-group">
                    <label for="GiveNameID">姓名</label>
                    <input type="text" class="form-control" id="GiveNameID" placeholder="姓名" name="GiveName" required/>
                </div>
                <div class="row">
                    <div class="col-md-2 col-md-offset-3">
                        <button type="submit" class="btn btn-default">确定</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
